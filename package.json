{
  "name": "Backpacker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "update:pods": "cd ios && pod install --repo-update && cd ..",
    "postinstall": "yarn run update:pods",
    "clean-android": "cd android && ./gradlew clean && cd ../ && yarn react-native run-android",
    "set-icon-android": "react-native set-icon --path ./app/global/assets/icon.png --platform android",
    "prettier": "prettier --write '**/*.{ts,js,css,html}'"
  },
  "dependencies": {
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/async-storage": "^1.10.0",
    "@react-native-community/clipboard": "^1.2.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.5.1",
    "@react-navigation/native": "^5.2.6",
    "@react-navigation/stack": "^5.3.1",
    "@twotalltotems/react-native-otp-input": "^1.3.7",
    "axios": "^0.19.2",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-appearance": "^0.3.4",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-gifted-chat": "^0.16.1",
    "react-native-paper": "^3.10.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-responsive-fontsize": "^0.4.3",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "crypto-js": "4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4
  }
}
